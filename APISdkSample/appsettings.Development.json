{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "bkssdk": {
    "LicenseKey": "BKS-2025-PREMIUM-KEY",
    "ApplicationName": "api-sdk-sample",
    "Redis": {
      "ConnectionString": "localhost:6379",
      "InstanceName": "bks-sdk",
      "Database": 0
    },
    "Jwt": {
      "SecretKey": "super-secret-jwt-key-bks-sdk-2025",
      "Issuer": "bks.sdk",
      "Audience": "api-sdk-sample",
      "ExpirationInMinutes": 60
    },
    "EventBroker": {
      "BrokerType": "RabbitMQ",
      "ConnectionString": "amqp://guest:guest@localhost:5672/",
      "AdditionalSettings": {
        "ExchangeName": "bks-sdk-events",
        "QueuePrefix": "bks-demo",
        "RetryAttempts": "3",
        "RetryDelay": "5000"
      }
    },
    "Observability": {
      "ServiceName": "api-sdk-sample",
      "ServiceVersion": "1.0.0",
      "JaegerEndpoint": "http://localhost:14268/api/traces"
    }
  }
}
